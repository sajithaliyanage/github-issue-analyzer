<?xml version="1.0" encoding="UTF-8"?>
<task class="org.apache.synapse.startup.tasks.MessageInjector" group="synapse.simple.quartz" name="DailyJobForCheckGitIssueState" xmlns="http://ws.apache.org/ns/synapse">
    <trigger cron="0 0 1 * * ?"/>
    <property name="message" xmlns:task="http://www.wso2.org/products/wso2commons/tasks">
        <echoString xmlns="">
            <in>DailyCronJobToCheckGitIssueStatus</in>
        </echoString>
    </property>
    <property name="injectTo" value="sequence" xmlns:task="http://www.wso2.org/products/wso2commons/tasks"/>
    <property name="sequenceName" value="DailyScheduleTask" xmlns:task="http://www.wso2.org/products/wso2commons/tasks"/>
</task>
